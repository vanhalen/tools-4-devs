        "/api/generator/certidao": {
            "get": {
                "tags": [
                    "Geradores"
                ],
                "summary": "Gera uma certidão válida",
                "description": "Gera uma certidão válida com base nos parâmetros informados, como tipo, estado, ano de registro e cartório. A certidão pode ser formatada ou não.",
                "operationId": "gerarCertidao",
                "parameters": [
                    {
                        "name": "type",
                        "in": "query",
                        "description": "Tipo de certidão a ser gerada. Padrão: nascimento. Valores permitidos: nascimento, casamento, obito, casamento-religioso.",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "nascimento",
                                "casamento",
                                "obito",
                                "casamento-religioso"
                            ],
                            "example": "nascimento"
                        }
                    },
                    {
                        "name": "uf",
                        "in": "query",
                        "description": "UF (sigla do estado) onde a certidão será registrada. Caso não informado, gera aleatoriamente.",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "AC",
                                "AL",
                                "AM",
                                "AP",
                                "BA",
                                "CE",
                                "DF",
                                "ES",
                                "GO",
                                "MA",
                                "MG",
                                "MS",
                                "MT",
                                "PA",
                                "PB",
                                "PE",
                                "PI",
                                "PR",
                                "RJ",
                                "RN",
                                "RO",
                                "RR",
                                "RS",
                                "SC",
                                "SE",
                                "SP",
                                "TO"
                            ],
                            "example": "SP"
                        }
                    },
                    {
                        "name": "year",
                        "in": "query",
                        "description": "Ano do registro da certidão. Caso não informado, gera aleatoriamente.",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 2023
                        }
                    },
                    {
                        "name": "notary",
                        "in": "query",
                        "description": "Código do cartório onde a certidão será registrada. Caso não informado, gera aleatoriamente.",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 123
                        }
                    },
                    {
                        "name": "formatted",
                        "in": "query",
                        "description": "Define se a certidão será formatada (true ou false). Padrão: true.",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Certidão gerada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "data": {
                                            "properties": {
                                                "certidao": {
                                                    "type": "string",
                                                    "example": "12345.67.89.0001.01234.5678.9012345-67"
                                                },
                                                "tipo": {
                                                    "type": "string",
                                                    "example": "nascimento"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erro ao gerar a certidão",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "errors": {
                                            "properties": {
                                                "message": {
                                                    "type": "string",
                                                    "example": "Erro ao gerar a certidão."
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/generator/senha": {
